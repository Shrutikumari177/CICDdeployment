							payload: {
                                bindingPath: '/xNAUTIxnewvend_btp',
                                searchKeys: ['Ort01', 'Land1', 'Lifnr']
							}
						}"
						p13nMode = "Item,Value">
					<mdc:basicSearchField>
						<mdc:FilterField delegate="{name: 'sap/ui/mdc/field/FieldBaseDelegate'}"
							dataType="sap.ui.model.type.String"
							placeholder= "Search Here"
							conditions="{$filters>/conditions/$search}"
							propertyKey="$search"
							maxConditions="1"/>
					</mdc:basicSearchField>
					<mdc:filterItems>
                        <mdc:FilterField
							label="Vendor No"
							propertyKey="Lifnr"
							dataType="sap.ui.model.type.String"
							conditions="{$filters>/conditions/Lifnr}"
							valueHelp="name-vh"
							delegate="{name: 'sap/ui/mdc/field/FieldBaseDelegate'}"
							/>

						<mdc:FilterField
							label="City"
							propertyKey="Ort01"
							dataType="sap.ui.model.type.String"
							conditions="{$filters>/conditions/Ort01}"
							valueHelp="name-vh"
							delegate="{name: 'sap/ui/mdc/field/FieldBaseDelegate'}"
							/>
						<mdc:FilterField
							label="Country"
							propertyKey="Land1"
							dataType="sap.ui.model.type.String"
							conditions="{$filters>/conditions/Land1}"
							valueHelp="name-vh"
							delegate="{name: 'sap/ui/mdc/field/FieldBaseDelegate'}"
							/>
                        
					</mdc:filterItems>
				</mdc:FilterBar>
			</f:DynamicPageHeader>
		</f:header>

