
<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"> 
    <SelectDialog
        id="mySelectDialog12"
        noDataText="Loading..."
        title="Select Voyage"
        search="onValueHelpSearch"
        confirm="onValueHelpClosevoy"
        cancel="onValueHelpClosevoy"
        
        items="{
        path: '/xNAUTIxvoy_valuehelp',
        sorter: {
            path: 'Voyno',
            descending: true
        }
        }"

        initialFocus="SearchField"
        liveChange="onValueHelpSearch"
  
         >
        <StandardListItem id="_IDGenStandardListItem1"
            title="{Voyno}"
       
            description="{voynm}"
       
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>